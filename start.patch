+
+    protected MenuItem mScrollableMenuItem;
+    protected MenuItem mPinchZoomMenuItem;
+    protected MenuItem mLogEventsMenuItem;
+
+    protected CodeGenerationRequest.CodeGeneratorCallback mCodeGeneratorCallback =
