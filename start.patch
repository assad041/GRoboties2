+        // TODO(#17): More complete character escaping: unicode characters to hex, octal, or \\u.
+        String escapedStr = str.replace("\\", "\\\\")  // Must escape backslashes first.
+                .replace("</", "<\\/")  // See: http://stackoverflow.com/a/6117915
+                .replace("\'", "\\\'")
+                .replace("\"", "\\\"")
+                .replace("\b", "\\b")
+                .replace("\f", "\\f")
+                .replace("\n", "\\n")
+                .replace("\r", "\\r")
+                .replace("\t", "\\t");
+        return "\"" + escapedStr + "\"";
+    }
